{
  "name": "AWS VPN tabs closer",
  "description": "Close the tabs automatically when authentication finished",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting", "tabs", "webNavigation"],
  "host_permissions": ["http://127.0.0.1:35001/*", "https://127.0.0.1:35001/*"],
  "action": {},
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
